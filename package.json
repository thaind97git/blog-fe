{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "The React Boilerplate with Webpack config manual",
  "main": "index.js",
  "scripts": {
    "postinstall": "node .postinstall",
    "test": "node config/jest/index.js --updateSnapshot  --watch",
    "start": "webpack serve --config webpack/webpack.dev.js --hot",
    "prod": "node server.js",
    "build": "webpack --config webpack/webpack.prod.js",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|ts|json|css|md)\"",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "lint-staged": {
    "*.+(js|jsx|ts)": "eslint --max-warnings 0 --fix",
    "*.+(js|jsx|ts|json|css|md)": "prettier --write"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "connected-react-router": "^6.9.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "i18next": "^19.9.2",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.21",
    "marked": "^2.0.3",
    "moment": "^2.29.1",
    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.9",
    "react-i18next": "^11.8.10",
    "react-icons": "^4.2.0",
    "react-paginate": "^7.1.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.3",
    "react-tooltip": "^4.2.17",
    "redux": "^4.0.5",
    "redux-api-call": "^1.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.4.3",
    "eslint": "^7.21.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "import-glob": "^1.5.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.2",
    "lint-staged": "^10.5.4",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^5.0.0",
